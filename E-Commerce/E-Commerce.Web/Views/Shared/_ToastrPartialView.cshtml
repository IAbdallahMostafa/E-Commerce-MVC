<!-- Toastr -->
<script src="~/plugins/jquery/jquery.min.js"></script>
<script src="~/js/toastr.min.js"></script>

@if (TempData["Create"] != null)
{
    <script type="text/javascript">       
        toastr["success"]("@TempData["Create"]")
    </script>
}

@if (TempData["Edit"] != null)
{
    <script type="text/javascript">
        toastr["info"]("@TempData["Edit"]")
    </script>
}

@if (TempData["Delete"] != null)
{
    <script type="text/javascript">
        toastr["error"]("@TempData["Delete"]")
    </script>
}

@if (TempData["CartAdded"] != null)
{
    <script type="text/javascript">
        toastr["success"]("@TempData["CartAdded"]")
    </script>
}

@if (TempData["OrderPlaced"] != null)
{
    <script type="text/javascript">
        toastr["success"]("@TempData["OrderPlaced"]")
    </script>
}

@if(TempData["UpdateOrderDetails"] != null)
{
    <script type="text/javascript">
        toastr["success"]("@TempData["UpdateOrderDetails"]")
    </script>

}
